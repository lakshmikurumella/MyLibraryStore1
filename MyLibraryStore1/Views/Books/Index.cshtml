@model IEnumerable<MyLibraryStore1.Models.Book>@{ ViewBag.Title = "Books"; }<h1>List Of Books</h1>@if (Model.Count() == 0){<h3>No Books Available</h3> }            else            {<ul class="list-group">    @foreach (var book in Model)    {<li class="list-group-item">    <a asp-controller="Books" asp-action="Details" asp-route-id="@book.Id">@book.BookName</a></li>}</ul>}